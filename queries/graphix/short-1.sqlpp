//     :param personId: 933

FROM   GRAPH SNB.Native.SNBGraph
MATCH  (person:Person)-[:IS_LOCATED_IN]->(city:City)
WHERE  person.id = $personId
SELECT person.firstName,
       person.lastName,
       person.birthday,
       person.locationIP,
       person.browserUsed,
       city.id,
       person.gender,
       person.creationDate;